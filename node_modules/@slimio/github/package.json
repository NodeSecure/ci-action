{
    "name": "@slimio/github",
    "version": "1.0.0",
    "description": "Download repository from github",
    "main": "index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && nyc --reporter=lcov ava --verbose test/test.js",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "codecov",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/github-download.git"
    },
    "keywords": [
        "SlimIO",
        "github",
        "download",
        "archive"
    ],
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/github-download/issues"
    },
    "homepage": "https://github.com/SlimIO/github-download#readme",
    "dependencies": {
        "@myunisoft/httpie": "^1.0.0",
        "@slimio/is": "^1.5.0",
        "tar-fs": "^2.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^4.1.0",
        "@slimio/psp": "^0.12.3",
        "ava": "^3.15.0",
        "codecov": "^3.8.3",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "husky": "^4.3.8",
        "jsdoc": "^3.6.7",
        "nyc": "^15.1.0",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
